{
  "name": "@schemasset/nuxt",
  "type": "module",
  "version": "0.0.5",
  "description": "Nuxt module for schemasset - schema-based asset file validation",
  "author": "Mates System",
  "license": "MIT",
  "homepage": "https://github.com/mates-system/schemasset/tree/main/packages/nuxt#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mates-system/schemasset"
  },
  "bugs": {
    "url": "https://github.com/mates-system/schemasset/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "asset",
    "schema",
    "validation",
    "file-structure",
    "static-assets"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "nuxt-module-build build"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.11.1",
    "@nuxt/module-builder": "^1.0.1",
    "@schemasset/core": "workspace:*",
    "@schemasset/utils": "workspace:*",
    "defu": "^6.1.4",
    "fast-glob": "^3.3.3"
  }
}
